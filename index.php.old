$sql = "SELECT * 
		FROM
		  jornadas AS j,
		  temporada AS t,
		  ligas AS l
		WHERE 
		  j.IDLiga = l.IDLiga AND
		  j.IDTemporada = t.IDTemporada AND
		  j.IDLiga = t.IDLiga AND
		  t.Status = '1' AND
		  j.Status = '1' AND
		  l.IDLiga = '$IDLiga'
		";
$consulta_jornada = mysql_query($sql);
$renglon_jornada = mysql_fetch_assoc($consulta_jornada);
